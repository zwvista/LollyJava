<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <table-filter match-name="VDICTIONARIES2"></table-filter>
    <table-filter match-name="VLANGUAGES2"></table-filter>
    <table-filter match-name="VTEXTBOOKS2"></table-filter>
    <table name="VDICTIONARIES2" class="com.zwstudio.lolly.domain.Dictionary">
        <primary-key>
            <key-column name="LANGIDFROM"/>
            <key-column name="DICTNAME"/>
        </primary-key>
    </table>
    <table name="VLANGUAGES2" class="com.zwstudio.lolly.domain.Language">
        <primary-key>
            <key-column name="LANGID"/>
        </primary-key>
    </table>
    <table name="VTEXTBOOKS2" class="com.zwstudio.lolly.domain.TextBook">
        <primary-key>
            <key-column name="TEXTBOOKID"/>
        </primary-key>
    </table>
</hibernate-reverse-engineering>